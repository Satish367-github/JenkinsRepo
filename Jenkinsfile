    stages {
        stage('Develop') {
            steps {
                echo 'Application Developed...'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Build is completed...'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Test is completed...'
            }
        }
        
        stage('Integration') {
            steps {
                echo 'Integration Test is completed...'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deployment is completed...'
            }
        }
    }
}
